    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-add-module-exports": "*",
    "babel-preset-es2015": "*",
    "chai": "*",
    "eslint": "*",
    "eslint-loader": "*",
    "mocha": "*",
    "webpack": "*",
    "yargs": "*"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
        "sourceType": "module"
    }
  },
  "babel": {
    "presets": ["es2015"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BuddhistDigitalResourceCenter/tibetan-sort-js.git"
  },
  "keywords": [
